<Application x:Class="PL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PL"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <local:EngineerExperienceCollection x:Key="EngineerExperienceCollectionKey" />
        <local:ConvertIdToContent x:Key="ConverIdToContentKey"/>
        <local:ConvertIdToIsEnabled x:Key="ConverIdToIsEnabledKey"/>
        <local:ConvertcreateSToVisible x:Key="ConvertcreateSToVisiblekey"/>
        <local:ConvertSeeMilestoneToVisible x:Key="ConvertSeeMilestoneToVisiblekey"/>


        <Style TargetType="Button">
            <Setter Property="FontSize"  Value ="32"/>
            <Setter Property="FontWeight"  Value ="Bold"/>
            <Setter Property="Height"  Value ="100"/>
            <Setter Property="Width"  Value ="200"/>

        </Style>
    </Application.Resources>
</Application>
