<Application x:Class="PL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PL"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <local:EngineerExperienceCollection x:Key="EngineerExperienceCollectionKey" />
        <local:StatusCollection x:Key="StatusCollectionKey" />
        <local:ConvertIdToContent x:Key="ConverIdToContentKey"/>
        <local:ConvertIdToIsEnabled x:Key="ConverIdToIsEnabledKey"/>
        <local:ConvertcreateBoolSToVisible x:Key="ConvertcreateSToVisiblekey"/>
        <local:ConvertBoolSeeMilestoneToVisible x:Key="ConvertSeeMilestoneToVisiblekey"/>
        <local:ConvertIdToVisbilty x:Key="ConvertIdToVisbiltykey"/>

        <Style TargetType="Button">
            <Setter Property="FontSize"  Value ="32"/>
            <Setter Property="FontWeight"  Value ="Bold"/>
            <Setter Property="Height"  Value ="100"/>
            <Setter Property="Width"  Value ="200"/>

        </Style>
        <Style TargetType="TextBox">
         
            <Setter Property="Height"  Value ="30"/>
            <Setter Property="Width"  Value ="200"/>
            <Setter Property="BorderBrush" Value="Yellow"/>

        </Style>
    </Application.Resources>
</Application>
